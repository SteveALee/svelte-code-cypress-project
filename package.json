{
	"name": "svelte-code-cypress-project",
	"version": "1.0.0",
	"author": "Steve Lee <steve@opendirective.com.com> (https://opendirective.com)",
	"description": "A template for svelte app with cypress and setup for VS Code",
	"license": "MIT",
	"private": true,
	"repository": "github:SteveALee/svelte-code-cypress-project.git",
	"devDependencies": {
		"cypress": "3.4.1",
		"eslint": "6.5.1",
		"eslint-config-prettier": "6.4.0",
		"eslint-plugin-cypress": "2.7.0",
		"eslint-plugin-prettier": "3.1.1",
		"eslint-plugin-svelte3": "2.7.3",
		"npm-run-all": "4.1.5",
		"prettier": "1.18.2",
		"prettier-plugin-svelte": "0.7.0",
		"rollup": "1.23.1",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-livereload": "1.0.4",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-svelte": "5.1.0",
		"rollup-plugin-terser": "5.1.2",
		"start-server-and-test": "^1.10.5",
		"svelte": "3.12.1"
	},
	"dependencies": {
		"sirv-cli": "0.4.4"
	},
	"scripts": {
		"build": "rollup -c",
		"autobuild": "rollup -c -w",
		"dev": "run-p start:dev autobuild",
		"start": "sirv public --single",
		"start:dev": "sirv public --single --dev",
		"format": "prettier --write src/*",
		"lint": "eslint src/*",
		"lint:fix": "eslint --fix src/*",
		"cy:run": "cypress run --spec \"cypress/integration/tdd.spec.js\"",
		"cy:open": "cypress open",
		"test": "start-server-and-test dev http://localhost:5000 cy:open"
	}
}
